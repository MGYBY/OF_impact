// rhoR 100.50;
// visR 67.0;
// slopeTan 0.050;
// frNum 0.80;
// reyNum #eval{3.0*$frNum*$frNum/$slopeTan};
// waterRho 1.0;
// airRho #eval{$waterRho/$rhoR};
// DebNum 1.0;
// rVis 0.50;
// etaTotal #eval{1.0/$reyNum};
// etaSVal #eval{$etaTotal*$rVis};
// etaPVal #eval{$etaTotal-$etaSVal};
// airEta #eval{$etaTotal/$visR};
// weNum 0.05; // use a slightly larger value

phases (water air);

water
{
      parameters
       {		        
         type             Oldroyd-BLog;
        // type             Oldroyd-BSqrt;
         rho              rho [ 1 -3 0 0 0 0 0 ] 1.0;
         etaS             etaS [1 -1 -1 0 0 0 0] 0.033333;
         etaP             etaP [1 -1 -1 0 0 0 0] 0.033333;
         lambda           lambda [0 0 1 0 0 0 0] 1.0;
         
         stabilization    coupling;
       }
}

air
{
     parameters
       {		        
         type            Newtonian;
         rho             rho [1 -3 0 0 0 0 0] 0.0098;
         eta             eta [1 -1 -1 0 0 0 0] 4.0e-4; 
       }
}
 

sigma           sigma [ 1 0 -2 0 0 0 0 ] 0.025;

