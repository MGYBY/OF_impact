FoamFile
{
    version     2.0;
    format      ascii;
    class       volSymmTensorField;
    object      tau.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0 0 0 0);

boundaryField
{
   inflow
    {
        // type            zeroGradient;
        type            fixedValue;
        value           uniform (0 0 0 0 0 0);
    }

    outflow
    {
        type            zeroGradient;
    }

    atmosphere
    {
        type            zeroGradient;
    }

    lowerWall
    {
        // type            zeroGradient;
        type            linearExtrapolation;
        value           uniform (0 0 0 0 0 0);
    }

    frontAndBack
    {
        type            empty;
    }
}
