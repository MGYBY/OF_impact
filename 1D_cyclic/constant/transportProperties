FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water air);

water
{
    transportModel  Newtonian;
    nu              1.003e-06;
    rho             1000.0;
}

air
{
    transportModel  Newtonian;
    nu              1.789e-05;
    rho             1.225;
}

sigma            0.0;

