FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(

  boxToCell
    {
        box (0 0 0) (1.0 0.00160557875 1);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
            volVectorFieldValue U ( 0.001 0 0 )
        );
    }
);


